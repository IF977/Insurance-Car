    <th><strong>ENDERECO</strong></th>
    <th><strong>TELEFONE</strong></th>
    <th>EXCLUIR CLIENTE</th>
  </tr>
  </thead>
  <tr>
    <% @consulta.each do |f| %>
    <%= form_for :cliente, :action => cliente_updateform_path do |j| %>
    <td><strong><%= j.text_field :cpf, :readonly => true, :value => f.cpf %></strong></td>
    <td><strong><%= f.nome%></strong></td>
    <td><strong><%= f.endereco %></strong></td>
    <td><strong><%= f.telefone %></strong></td>
    <td><%= j.submit "DELETAR CLIENTE"%></td>
    <% end %>
  </tr>
<% end %>
</p>
